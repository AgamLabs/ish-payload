{
  "name": "@payloadcms/template-ecommerce",
  "version": "1.0.0",
  "description": "Ecommerce template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "db:create": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts NODE_ENV=development payload create:db",
    "db:migrate": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload migrate",
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--no-deprecation SKIP_LINT=true next build",
    "dev": "cross-env NODE_ENV=development next dev",
    "lint": "next lint --max-warnings 0",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_ENV=production NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@payloadcms/db-postgres": "3.23.0",
    "@payloadcms/db-vercel-postgres": "3.23.0",
    "@payloadcms/email-nodemailer": "3.23.0",
    "@payloadcms/live-preview-react": "3.23.0",
    "@payloadcms/next": "3.23.0",
    "@payloadcms/payload-cloud": "3.23.0",
    "@payloadcms/plugin-cloud-storage": "3.23.0",
    "@payloadcms/plugin-form-builder": "3.23.0",
    "@payloadcms/plugin-nested-docs": "3.23.0",
    "@payloadcms/plugin-redirects": "3.23.0",
    "@payloadcms/plugin-search": "3.23.0",
    "@payloadcms/plugin-seo": "3.23.0",
    "@payloadcms/richtext-lexical": "3.23.0",
    "@payloadcms/storage-vercel-blob": "3.23.0",
    "@payloadcms/translations": "3.23.0",
    "@payloadcms/ui": "3.23.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@vercel/blob": "^0.27.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.9",
    "embla-carousel-auto-scroll": "^8.1.5",
    "embla-carousel-react": "^8.1.5",
    "express": "^5.1.0",
    "geist": "^1.3.0",
    "graphql": "^16.10.0",
    "jsonwebtoken": "9.0.1",
    "juice": "^9.1.0",
    "lucide-react": "^0.378.0",
    "next": "15.2.4",
    "nodemailer": "6.9.10",
    "payload": "^3.23.0",
    "payload-admin-bar": "^1.0.6",
    "prism-react-renderer": "^2.3.1",
    "qs-esm": "^7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-fast-marquee": "^1.6.5",
    "react-hook-form": "7.45.4",
    "sharp": "0.32.6",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.12",
    "@types/ejs": "^3.1.5",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "22.5.4",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "@vercel/git-hooks": "^1.0.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "lint-staged": "^15.4.3",
    "pg": "^8.13.3",
    "postcss": "^8.4.38",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
